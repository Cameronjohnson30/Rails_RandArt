<h1>Styles</h1>

<% if @styles.any? %> 
  <ul>
  <% @styles.each do |style| %>
    <li><%= link_to style.name, style_path(style) %></li>
    <% end %>
    </ul>
    <% else %>
    <p>There are no styles yet.</p>
    <% end %>

    <p><%= button_to "Create new style",
                  new_style_path, 
                  method: :get %></p>
<h1>Your Random Art Project Is.... </h1>
<h2><btn><%= random(@style)%> </btn></h2>

